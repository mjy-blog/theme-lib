{
  "private": true,
  "name": "@mjy-blog/theme-lib",
  "version": "0.1.3",
  "description": "mjy-blog theme library",
  "types": "./index.d.ts",
  "main": "./index.js",
  "scripts": {
    "build": "rimraf compile dist && tsc -p tsconfig.compile.json && rollup -c && dts-bundle-generator --no-banner -o dist/index.d.ts compile/index.d.ts",
