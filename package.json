  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "cpy-cli": "^5.0.0",
    "dts-bundle-generator": "^8.0.1",
    "package.json-minifier": "^0.0.4",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "@types/react": "^18"
  }
